{
  "name": "defisaver-tokens",
  "version": "1.3.8",
  "description": "Centralized asset data & utility functions\n",
  "type": "module",
  "main": "umd/main.js",
  "module": "esm/index.js",
  "scripts": {
    "version-bump": "git commit -am \"Version bump to $(npm version patch --no-git-tag-version | cut -c 2-)\"",
    "update-ilks": "node scripts/updateIlksInfo",
    "document": "jsdoc2md --param-list-format list --files index.mjs utils.mjs > README.md",
    "build:umd": "rm -rf umd && webpack --config=webpack.umd.js",
    "build:esm": "rm -rf esm && babel src --out-dir esm",
    "build": "npm run build:umd && npm run build:esm"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "decimal.js": "^10.2.1",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "jsdoc-to-markdown": "^6.0.1",
    "webpack": "^5.12.3",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
  }
}
